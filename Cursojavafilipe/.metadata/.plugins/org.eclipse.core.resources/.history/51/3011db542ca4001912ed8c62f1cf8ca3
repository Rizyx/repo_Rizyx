package br.com.orientacao_a_objetos;

public class Turma {
	private String periodo = null;
	private int serie = 0;
	private String sigla = null;
	private String tpensino = null;
	
	public String getPeriodo() {
		return periodo;
	}
	public void setPeriodo(String periodo) {
		this.periodo = periodo;
	}
	public int getSerie() {
		return serie;
	}
	public void setSerie(int serie) {
		this.serie = serie;
	}
	public String getSigla() {
		return sigla;
	}
	public void setSigla(String sigla) {
		this.sigla = sigla;
	}
	public String getTpensino() {
		return tpensino;
	}
	public void setTpensino(String tpensino) {
		this.tpensino = tpensino;
	}
	
	
 	}


